    "@types/koa-compress": "^2.0.9",
    "@types/koa-joi-router": "^5.2.3",
    "@types/koa-joi-router-docs": "^1.0.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa-useragent": "^2.1.2",

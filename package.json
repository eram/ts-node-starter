{
  "name": "ts-node-starter",
  "version": "1.0.0",
  "description": "ts-node starter project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/eram/ts-node-starter.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint --fix src/**/*.ts && echo 'Lint complete.'",
    "clean": "node node_modules/rimraf/bin coverage *.log logs dist ~/.pm2-dev/logs ~/.pm2/logs ~/pm2/*log",
    "start:node": "node src",
    "dev": "pm2-dev ./pm2.json --env \"\" -i 1",
    "start": "pm2 start --env production ts-node -- -P pm2.json ./src/index.ts"
  },

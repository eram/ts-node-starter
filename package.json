{
  "name": "ts-node-starter",
  "version": "2.0.0",
  "description": "ts-node starter project",
  "main": "index.js",
  "private": "false",
  "repository": {
    "type": "git",
    "url": "https://github.com/eram/ts-node-starter.git"
  },
  "author": "eram",
  "license": "ISC",
  "scripts": {
    "test": "eslint --color src/**/*.ts && jest --coverage --silent",
    "lint": "eslint --color --fix src/**/*.ts && echo Lint complete.",
    "clean": "rimraf coverage *.log logs dist coverage .database.sqlite",
    "jest": "jest --watch",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --rs --inspect -- src",
    "dev:cluster": "ts-node-dev --respawn --transpile-only --exit-child --rs --inspect -- src cluster ./cluster.config.js",
    "start": "node src cluster ./cluster.config.js",
    "start:node": "node src job src/jobs/app",
    "createDb": "node src job src/jobs/createDb",
    "build": "npm run clean && tsc"
  },
  "config": {},
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@koa/cors": "^3.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-helmet": "^6.0.1",
    "@types/koa-joi-router": "^5.2.5",
    "@types/koa-joi-router-docs": "^1.0.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^3.0.2",
    "@types/node": "^12.19.11",
    "axios": "^0.21.1",
    "commander": "^6.2.1",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^6.0.0",
    "koa-joi-router": "^6.0.2",
    "koa-joi-router-docs": "^1.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa-useragent": "^4.0.0",
    "node-pre-gyp": "^0.14.0",
    "sequelize": "^6.3.5",
    "trie-mapping": "^3.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "rimraf": "^3.0.2",
    "sqlite3": "^4.2.0",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1"
  }
}
